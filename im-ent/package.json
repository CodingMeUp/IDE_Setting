{
  "name": "IM-ENT",
  "version": "1.0.0",
  "description": "IM Enterprise Console",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "author": "vnot <weinotme@gmail.com>",
  "license": "MIT",
  "scripts": {
    "gfw": "npm i cross-env && cross-env PHANTOMJS_CDNURL=http://npm.taobao.org/mirrors/phantomjs npm install -r https://registry.npm.taobao.org",
    "clean": "rimraf ../webapp",
    "compile": "node -r dotenv/config --harmony dev/bin/compile",
    "lint": "eslint . ./app",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "server": "http-server ../webapp -p 8000 -o",
    "lock": "npm shrinkwrap --dev"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon -r dotenv/config dev/bin/server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node -r dotenv/config dev/bin/server"
    },
    "test": {
      "command": "node -r dotenv/config ./node_modules/karma/bin/karma start dev/bin/karma.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
